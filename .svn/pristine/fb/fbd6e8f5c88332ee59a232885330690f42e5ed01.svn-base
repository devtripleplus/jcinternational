#eAsia .eAsiaIn{margin-top: 40px;}
#eAsia .sectionHdr p{font-style: italic;color: #9aa1a7;}
#eAsia .eAsiaIn .eAsiaInL{width: 55%;display: inline-block;vertical-align: middle;}
#eAsia .eAsiaIn .eAsiaInR{width: 40%;display: inline-block;padding-left: 0px;vertical-align: middle;}
#eAsia .eAsiaIn .eAsiaInfo {display: inline-block;vertical-align: middle;}
#eAsia .eAsiaIn .eAsiaInfo h6{margin: 5px 0;}
#eAsia .eAsiaIn .eAsiaInfo p{margin: 0;}
#eAsia .eAsiaIn .eAsiaInfo p.txt-14{color: #5e5e5e;}
#eAsia .eAsiaIn .eAsiaInfo p.txt-24{color: #2d3092; font-family: 'Avenir Medium'; }
#eAsia .eAsiaIn .eAsiaInfo p.txt-24>span{color: #2d3092;font-family: 'Avenir Black'; }
#eAsia .eAsiaIn .eAsiaInfo a{color: #ed1c24;font-family: 'Avenir Medium'; text-decoration: underline;}

#eAsia .eAsiaIn  .eAsiaImg{display: inline-block;vertical-align: middle;width: 140px;height: 140px;border-radius: 50%;overflow: hidden;}
#eAsia .eAsiaIn  .eAsiaImg img{display: block;margin: 0 auto;width: 100%;height: 100%;}

#eAsia .owl-dots{display: block;text-align: center;margin-top: 40px;}
