@charset "utf-8";
/* <<HEADER>> */
.mainHeader {
   padding: 0 0 15px 0;
        border-bottom: 2px solid #cdb9d2;
}


.nopadding{padding: 0 !important;}
.nopaddingLR{padding-left: 0 !important;padding-right: 0 !important;}

.mainHeader ul li{font-weight: 300;display: inline-block;font-size: 16px;padding: 30px 15px;position: relative;}
.mainHeader ul li>a{    font-size: 18px;
    color: #283891;
    font-weight: 300;
    padding: 0 5px;
    transition: all 0.2s linear;
    font-family: 'Avenir Medium';
}

.mainHeader ul li>a:hover{    color: #ed1c24;    background-color: transparent;}
.mainHeader ul li>a.active{background: none;}
.mainHeader .navbar-brand{padding:15px 0;height:auto;display: inline-block;margin: 0 auto;}



.mainHeader ul li > div.innerSearch{position: absolute;top: 60px;right: 20px;display: none;width: 244px;z-index: 9999;}

.mainHeader ul li select{background-color: transparent;border: 1px solid #7a7ba5;color: #8293a5;font-size: 14px;background-position: top 17px right 10px;width: 54px;border-radius: 3px;margin-top: -10px;}


#myUL ,
#myUL2 {
  list-style-type: none;
  padding: 0;
  margin: 0;
  display: none;
  position: absolute;
  top: 40px;
  left: 0;
  min-width: 100%;
  z-index: 9999;
}

#myUL li a,
#myUL2 li a{
  border: 1px solid #ddd;
  margin-top: -1px; /* Prevent double borders */
  background-color: #f6f6f6;
  padding: 12px 9px;
  text-decoration: none;
  font-size: 13px;
  color: black;
  display: block
}

#myUL li a.header ,
#myUL2 li a.header {
  background-color: #e2e2e2;
  cursor: default;
}

#myUL li a:hover:not(.header),
#myUL2 li a:hover:not(.header){
  background-color: #eee;
}

.mainHeader ul li > div.innerSearch input{    border:1px solid #829384;
    min-height: 27px;
    max-height: 27px;
    border-radius:0;
    display:inline-block;
    }

.mainHeader ul li > div.innerSearch input[type="search"]{
    width:160px;
    }
.mainHeader ul li > div.innerSearch input[type="submit"]{
    background-color:#333;
    color:#fff;
    border:none;
    width: 70px;
    text-align: center;
    padding: 0;
    }



@media(max-width:1199px){
    .mainHeader .navbar-brand{float: none;}
