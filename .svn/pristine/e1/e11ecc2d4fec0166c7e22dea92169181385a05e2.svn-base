/*=========Map Location===========*/

.locationDiv{width:325px;padding: 0px 25px;font-family: 'Avenir Medium'!important;padding-right: 0;}
.locationDiv h3{color:#2e3192;font-size: 18px;font-family: 'Avenir Medium'!important;font-weight: 800;}
.locationDiv h2{color:#ec1c23;font-size: 24px;font-weight: bold;}
.locationDiv p{font-size: 16px;line-height: 30px;font-family: 'Avenir Medium'!important;margin-top: 15px;}
.locationDiv h1{font-size: 35px;color: #2d3092}

.locationDiv h1 a{float: right;font-size: 25px;color: #ec1c23;margin-top: 8px;font-weight: bold;}

.nomargin{margin: 0 !important;}

.manageBooking button,
.manageBooking input{
    min-width: 250px !important;
    font-size: 18px !important;
    min-height: 46px;
    letter-spacing: 1px;
    font-family: 'Avenir Medium'!important;
    font-weight: 800;
}

.flightSheduleForm p{
    clear: both;
    margin-bottom: 15px !important;
    display: inline-block;
    }

.flightSheduleForm p label{float: left;width: 158px;color: #2e3192;padding: 5px 0;} 
.flightSheduleForm p>span{float: left;width: 328px;} 

.flightSheduleForm p span input[type='checkbox']{display: none;}
.flightSheduleForm p span input[type='checkbox'] + label{background: url(../img/checkbox.png) no-repeat center left;padding: 20px;}
.flightSheduleForm p span input[type='checkbox']:checked + label{background: url(../img/checkbox-checked.png) no-repeat center left;}

.imgCentergs img{margin: 0 auto;}

@media(max-width:520px){
    .flightSheduleForm p>span{width: 100%;}
    .flightSheduleForm p{display: block;}
}

