/*======About Us============*/

#aboutPage .sectionHdr a{color:#ed1c24;font-size: 18px;font-weight: bold;line-height: 24px;text-decoration: none !important;}
#aboutPage .sectionHdr a img{margin-top: -3px;margin-right: 20px;transition-duration: 0.5s;}
#aboutPage .sectionHdr a:hover img{margin-right: 10px;}
#aboutPage .sectionInfo p{margin-bottom: 47px;}
#aboutPage .sectionInfo p{font-size: 18px;color: #2f2f2f;}
#aboutPage .sectionInfo .aboutImg {margin-bottom: 30px;}
#aboutPage .sectionInfo .aboutImg img{max-width: 100%;width: auto;height: auto;max-height: 100%;}
#aboutPage .sectionInfo .aInfoHdr{margin-bottom: 30px;position: relative;}
#aboutPage .sectionInfo .aInfoHdr h3 > a{position: static;float: right;display: inline-block;}

.sideBar{margin-top: 40px;}
.sideBar .widget{padding: 30px;margin-bottom: 40px;}
.redListIcon li{background: url(../img/list-red-icon.png) no-repeat left center;padding-left: 30px;line-height: 48px;font-size: 18px;}
.lightGray{background: #f3f5f6;}

.sideBar .widget h3{
    color: #ed1c24;
    font-size: 24px;
    padding-left: 80px;    
    margin-bottom: 20px;
    min-height: 55px;
    display: table;
}

.sideBar .widget h3 span{    
    display: table-cell;
    vertical-align: bottom;
    }

.sideBar .widget p{font-size: 18px;line-height: 30px;min-height: 170px;}
.sideBar .widget h3.wifiIcon{background: url(../img/wifi-icon.png) no-repeat left top;}
.sideBar .widget h3.entertainment{background: url(../img/entertain-video-icon.png) no-repeat left top;}

.sideBar .widget h6 a{font-size: 18px;color:#ed1c24;border-bottom: 1px solid #ed1c24;padding:0px 0;line-height: 26px;}

.topHeading{padding: 15px 0 0 0 !important;}
.topHeading .sectionHdr{margin-top: 20px;}
.topHeading .sectionHdr hr{background: none !important;margin: 0 !important;border: 0 !important;}
.topHeading .sectionHdr h1,
.topHeading .sectionHdr a{font-weight: bold;}

.btm-margin25{margin-bottom: 25px;}
.btm-margin40{margin-bottom: 40px;}
.refineRowgs p.refineGs{
    font-size: 18px;
    color: #2f2f2f;
    line-height: 30px;
    margin: 16px 0 30px 0;
    font-weight: 400;
}

.refineRowgs p.refineGs span{margin:0 10px;font-family: 'Avenir Medium';}
.refineRowgs p.refineGs span:first-child{margin-left: 0px;}
.refineRowgs p.refineGs span select.formInput{
    padding: 5px 35px 5px 15px;
    text-align-last: center;color:#2e3192;border: 1px solid #899fa9 !important;width: auto !important;display: inline;
    background-image: url(../img/iconDownArrow.png);    
    font-family: 'Avenir Medium';
    margin-bottom: 5px;
}

.FlightDetail table th,
