   #newsSocial .sectionHdr h3 small{display: block;} 
}

footer{background: rgba(46,49,147,1);background: -moz-linear-gradient(top, rgba(46,49,147,1) 0%, rgba(46,49,147,1) 83%, rgba(56,48,141,1) 85%, rgba(134,41,97,1) 100%);background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(46,49,147,1)), color-stop(83%, rgba(46,49,147,1)), color-stop(85%, rgba(56,48,141,1)), color-stop(100%, rgba(134,41,97,1)));background: -webkit-linear-gradient(top, rgba(46,49,147,1) 0%, rgba(46,49,147,1) 83%, rgba(56,48,141,1) 85%, rgba(134,41,97,1) 100%);background: -o-linear-gradient(top, rgba(46,49,147,1) 0%, rgba(46,49,147,1) 83%, rgba(56,48,141,1) 85%, rgba(134,41,97,1) 100%);background: -ms-linear-gradient(top, rgba(46,49,147,1) 0%, rgba(46,49,147,1) 83%, rgba(56,48,141,1) 85%, rgba(134,41,97,1) 100%);background: linear-gradient(to bottom, rgba(46,49,147,1) 0%, rgba(46,49,147,1) 83%, rgba(56,48,141,1) 85%, rgba(134,41,97,1) 100%);filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2e3193', endColorstr='#862961', GradientType=0 );}

