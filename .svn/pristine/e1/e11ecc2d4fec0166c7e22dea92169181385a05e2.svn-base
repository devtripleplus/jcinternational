@charset "utf-8";
/* <<HEADER>> */
.mainHeader {
   padding: 0 0 15px 0;
        border-bottom: 2px solid #cdb9d2;
}


.nopadding{padding: 0 !important;}
.nopaddingLR{padding-left: 0 !important;padding-right: 0 !important;}

.mainHeader ul li{font-weight: 300;display: inline-block;font-size: 16px;padding: 30px 15px;}
.mainHeader ul li>a{    font-size: 18px;
    color: #283891;
    font-weight: 300;
    padding: 0 5px;
    transition: all 0.2s linear;
    font-family: 'Avenir Medium';
}

.mainHeader ul li>a:hover{    color: #ed1c24;    background-color: transparent;}
.mainHeader ul li>a.active{background: none;}
.mainHeader .navbar-brand{padding:15px 0;height:auto;display: inline-block;margin: 0 auto;}

.mainHeader ul li select{background-color: transparent;border: 1px solid #7a7ba5;color: #8293a5;font-size: 14px;background-position: top 17px right 10px;width: 54px;border-radius: 3px;margin-top: -10px;}

@media(max-width:1199px){
    .mainHeader .navbar-brand{float: none;}
    .mainHeader .navbar-header{display: block;margin: 0 auto;width: 100%;text-align: center;}
    .mainHeader .navbar-nav{float: none!important;text-align: center;display: inline-block;}
    .mainHeader .navbar-collapse{text-align: center;}
    .page-breadcrumb .breadcrumb{text-align: center;}
}

@media(max-width:991px){
.mainHeader ul li>a {
        font-size: 16px;
    padding: 15px 15px;
}
}
@media(max-width:767px){
.mainHeader .navbar-nav {
        float: none!important;
    text-align: center;
    display: block;
    background-color: #efefef;
    padding: 0;
}
.mainHeader .navbar-brand {float: left;}
.mainHeader .navbar-brand img{width:200px;}    
.mainHeader ul li{display: block;float: none;padding: 15px 10px;}   
.navbar-toggle {
        position: relative;
    float: right;
    margin-top: 15px;
    margin-right: 15px;
    margin-bottom: 8px;
    background-color: rgb(46, 49, 146);
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
} 
.navbar-toggle .icon-bar {display: block;width: 22px;height: 2px;border-radius: 1px;background-color: #fff;}
.mainHeader #mobileNav{background-color: transparent;float: right!important;margin-right: 10px;}
.mainHeader #mobileNav li{display: inline-block;padding: 15px 5px;}
}

@media(max-width:600px){
    .top-header nav.nav-2{display: none;}
}

/* <<BANNER>> */

#banner {background-image: url(../img/banners/1.png);-webkit-background-size: cover;
background-size: cover;padding: 200px 0 100px 0;}
#banner #bannerTtl {margin-bottom: 40px;}
#banner #bannerTtl img{display: block;margin: 0 auto;}
#banner .bannerForm .bannerTop{background-image: url(../img/bannerFormBg.png);background-repeat: no-repeat;position: relative;}
#banner .bannerForm .bannerTop .bannerTopIn{position: absolute;top: 0;left: 0;padding-left: 20px;padding-top: 10px;}
#banner .bannerForm .bannerTop .bannerTopIn img{margin-right: 10px;display: inline-block;vertical-align: middle;}
#banner .bannerForm .bannerTop .bannerTopIn span{font-size: 18px;color: #2e3192;display: inline-block;vertical-align: middle; font-family: 'Avenir Medium'!important;}
#banner .bannerForm .bannerBottom{padding: 15px 10px;background-color: #fff;}
#banner .bannerForm .bannerBottom .bannerCols{float: left;padding: 0 10px;}
#banner .bannerForm .bannerBottom .bannerCols.bannerCols21{width: 21%;position: relative;}
#banner .bannerForm .bannerBottom .bannerCols.bannerCols16{width: 16%;position: relative;}
#banner .bannerForm .bannerBottom .c-chkbox{width: 50%;float: left;margin-top: 7px;}
#banner .bannerForm .bannerBottom .c-chkbox label{color: #2e3192;}

@media(max-width:1199px){
    #banner {padding: 100px 0 100px 0;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols21{width: 27%;position: relative;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols1{width: 100%;padding-bottom: 11px;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols1 .c-chkbox{width: 15%;float: left;}

}

@media(max-width:991px){
    #banner {padding: 100px 0 100px 0;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols21{width: 33%;position: relative;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols1{width: 100%;padding-bottom: 11px;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols1 .c-chkbox{width: 15%;float: left;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols16{width: 16%;position: relative;padding-top:15px}
    #banner #bannerTtl img{display: block;margin: 0 auto;}
}



@media(max-width:767px){
    #banner #bannerTtl img{width:300px}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols1 .c-chkbox{width: 25%;float: left;}
}

@media(max-width:480px){
    #banner {padding: 50px 0;}
    #banner #bannerTtl img{width:200px;margin-bottom:20px}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols1 .c-chkbox{width: 50%;float: left;}
    #banner .bannerForm .bannerBottom .bannerCols.bannerCols21{width: 100%;position: relative;margin-bottom:10px}
}



#sOffers .sOffersIn{margin-top: 40px;}
#sOffers .sOffersIn .sOffersTtl{}
